    "configurations": [
        {
            "name": "PowerShell Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${file}"
        },
        {
            "name": "healthcheckStatus with parameters",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}\\healthcheckStatus.ps1",
            "args": ["-env dev -serverType pdfsvc -outputIpAddresses"],
            "cwd": "${workspaceFolder}"
        }
    ]
